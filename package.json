{
  "name": "babel-node-5-config",
  "version": "1.0.0",
  "description": "A babel configuration for node 5.6.0 (async/await, more)",
  "main": "index.js",
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-plugin-transform-remove-console": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015-node5": "^1.1.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "colors": "^1.1.2"
  },
  "devDependencies": {},
  "babel": {
    "presets": ["es2015-node5", "stage-0"],
    "plugins": [
      "transform-runtime",
      "transform-strict-mode"
    ],
    "env": {
      "production": {
        "plugins": ["transform-remove-console"]
      }
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nbeydoon/babel-node-5-config.git"
  },
